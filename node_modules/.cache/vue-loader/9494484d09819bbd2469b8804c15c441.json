\r\n      },\r\n    ];\r\n    const item = ref({});\r\n    const index = ref(0);\r\n\r\n    item.value = cities[index.value];\r\n    const refresh = (item) => {\r\n      item.value = item;\r\n    };\r\n    const prev = () => {\r\n      index.value--;\r\n      index.value = index.value < 0 ? cities.length - 1 : index.value;\r\n      item.value = cities[index.value];\r\n    };\r\n    const next = () => {\r\n      index.value++;\r\n      index.value = index.value >= cities.length ? 0 : index.value;\r\n      item.value = cities[index.value];\r\n    };\r\n    const isRefresh = (item) => {\r\n      debugger;\r\n      if (item.value && item.value.id == item.id) {\r\n        return true;\r\n      } else {\r\n        return false;\r\n      }\r\n    };\r\n\r\n    const getsrc = (item) => {\r\n      let src = weatherType.value.find((x) => x.id == item.weatherType).src;\r\n      return `images/${src}`;\r\n    };\r\n    const weatherType = ref([\r\n      {\r\n        id: 1,\r\n        src: \"cloudy.png\",\r\n      },\r\n      {\r\n        id: 2,\r\n        src: \"rain.png\",\r\n      },\r\n      {\r\n        id: 3,\r\n        src: \"snow.png\",\r\n      },\r\n      {\r\n        id: 4,\r\n        src: \"sunny.png\",\r\n      },\r\n      {\r\n        id: 5,\r\n        src: \"thunderStorm.png\",\r\n      },\r\n    ]);\r\n\r\n    return {\r\n      index,\r\n      item,\r\n      isRefresh,\r\n      getsrc,\r\n      weatherType,\r\n      cities,\r\n      refresh,\r\n      prev,\r\n      next,\r\n    };\r\n  },\r\n});\r\n</script>\r\n\r\n\r\n<style>\r\n.refresh {\r\n  width: 77px;\r\n  height: 74px;\r\n  position: absolute;\r\n  top: 120px;\r\n  left: 700px;\r\n  cursor: pointer;\r\n}\r\n.prev {\r\n  width: 77px;\r\n  height: 74px;\r\n  position: absolute;\r\n  top: 120px;\r\n  left: 514px;\r\n  cursor: pointer;\r\n}\r\n.next {\r\n  width: 77px;\r\n  height: 74px;\r\n  position: absolute;\r\n  top: 120px;\r\n  right: 513px;\r\n  cursor: pointer;\r\n}\r\nh3 {\r\n  position: absolute;\r\