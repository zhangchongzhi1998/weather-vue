e + \"px;\";\r\n        let height = \"height:\" + heightValue + \"px;\";\r\n        styleName = styleName + fontSize  + height;\r\n      }\r\n      if (this.color) {\r\n        let color = \"color:\" + this.color + \";\";\r\n        styleName = styleName + color;\r\n      }\r\n      if (this.bgColor) {\r\n        let bgColor = \"background-color:\" + this.bgColor + \";\";\r\n        styleName = styleName + bgColor;\r\n      }\r\n      if (this.radius) {\r\n        let radius = \"border-radius:10px;\";\r\n        styleName = styleName + radius;\r\n      }\r\n      return styleName;\r\n    },\r\n    btnClassName() {\r\n      let className = \"\";\r\n      switch (this.type) {\r\n        case \"default\":\r\n          className = \"zlp-button-default\";\r\n          break;\r\n        case \"light\":\r\n          className = \"zlp-button-light\";\r\n          break;\r\n        case \"dark\":\r\n          className = \"zlp-button-dark\";\r\n          break;\r\n        case \"running\":\r\n          className = \"zlp-button-running\";\r\n          break;\r\n        case \"success\":\r\n          className = \"zlp-button-success\";\r\n          break;\r\n        case \"wait\":\r\n          className = \"zlp-button-wait\";\r\n          break;\r\n        case \"warning\":\r\n          className = \"zlp-button-warning\";\r\n          break;\r\n        case \"fail\":\r\n          className = \"zlp-button-fail\";\r\n          break;\r\n        case \"stop\":\r\n          className = \"zlp-button-stop\";\r\n          break;\r\n        case \"warm\":\r\n          className = \"zlp-button-warm\";\r\n          break;\r\n        case \"skyblue\":\r\n          className = \"zlp-button-skyblue\";\r\n          break;\r\n        case \"chinared\":\r\n          className = \"zlp-button-chinared\";\r\n          break;\r\n        case \"green\":\r\n          className = \"zlp-button-green\";\r\n          break;\r\n        case \"add\":\r\n          className = \"zlp-button-add\";\r\n     