= 50;\r\n            break;\r\n        }\r\n        let fontSize = \"font-size:\" + sizeValue + \"px;\";\r\n        let width = \"width:\" + widthValue + \"px;\";\r\n        let height = \"height:\" + heightValue + \"px;\";\r\n        styleName = styleName + fontSize  + height;\r\n      }\r\n      if (this.color) {\r\n        let color = \"color:\" + this.color + \";\";\r\n        styleName = styleName + color;\r\n      }\r\n      if (this.bgColor) {\r\n        let bgColor = \"background-color:\" + this.bgColor + \";\";\r\n        styleName = styleName + bgColor;\r\n      }\r\n      if (this.radius) {\r\n        let radius = \"border-radius:10px;\";\r\n        styleName = styleName + radius;\r\n      }\r\n      return styleName;\r\n    },\r\n    btnClassName() {\r\n      let className = \"\";\r\n      switch (this.type) {\r\n        case \"default\":\r\n          className = \"zlp-button-default\";\r\n          break;\r\n        case \"light\":\r\n          className = \"zlp-button-light\";\r\n          break;\r\n        case \"dark\":\r\n          className = \"zlp-button-dark\";\r\n          break;\r\n        case \"running\":\r\n          className = \"zlp-button-running\";\r\n          break;\r\n        case \"success\":\r\n          className = \"zlp-button-success\";\r\n          break;\r\n        case \"wait\":\r\n          className = \"zlp-button-wait\";\r\n          break;\r\n        case \"warning\":\r\n          className = \"zlp-button-warning\";\r\n          break;\r\n        case \"fail\":\r\n          className = \"zlp-button-fail\";\r\n          break;\r\n        case \"stop\":\r\n          className = \"zlp-button-stop\";\r\n          break;\r\n        case \"warm\":\r\n          className = \"zlp-button-warm\";\r\n          break;\r\n        case \"skyblue\":\r\n          className = \"zlp-button-skyblue\";\r\n          break;\r\n        case \"chinared\":\r\n          className = \"zlp-button-chinared\";\r\n          break;\r\n        case \"green\":\r\n          className = \"zlp-button-green\";\r\n          break;\r\n        case \"add\":\r\n          className = \"zlp-button-add\";\r\n          break;\r\n        case \"edit\":\r\n          className = \"zlp-button-edit\";\r\n          break;\r\n        case \"delete\":\r\n          className = \"zlp-button-delete\";\r\n          break;\r\n        case \"view\":\r\n          className = \"zlp-button-view\";\r\n          break;\r\n\r\n        case \"text\":\r\n          className = \"zlp-button-text\";\r\n          break;\r\n        case \"link\":\r\n          className = \"zlp-button-link\";\r\n          break;\r\n        default:\r\n          break;\r\n      }\r\n      return className;\r\n    },\r\n  },\r\n  methods: {\r\n    click(e) {\r\n      this.$emit(\"click\", e);\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.zlp-button {\r\n  display: inline-block;\r\n  padding: 5px;\r\n}\r\nbutton {\r\n  display: inline-block;\r\n  /* padding: 10px 20px; */\r\n  padding: 0 18px;\r\n  height: 38px;\r\n  cursor: pointer;\r\n  border: none;\r\n  background-color: transparent;\r\n  outline: none;\r\n  transition: 0.2s;\r\n  text-align: center;\r\n  box-sizing: border-box;\r\n  white-space: nowrap;\r\n  border: 1px solid #dcdfe6;\r\n}\r\n.zlp-button i {\r\n  color: #fff;\r\n  margin: 0 5px;\r\n  font-size: 1.5em;\r\n  vertical-align: middle;\r\n}\r\nbutton:hover {\r\n  opacity: 0.7;\r\n}\r\n.zlp-button-default {\r\n  background-color: #4caf50;\r\n  color: #fff;\r\n}\r\n.zlp-button-light {\r\n  background-color: #fff;\r\n  color: #000;\r\n}\r\n.zlp-button-dark {\r\n  background-color: #000;\r\n  color: #fff;\r\n}\r\n.zlp-button-running {\r\n  background-color: #4caf50;\r\n  color: #fff;\r\n}\r\n.zlp-button-success {\r\n  background-color: #4caf50;\r\n  color: #fff;\r\n}\r\n.zlp-button-stop {\r\n  background-color: #f44336;\r\n  color: #fff;\r\n}\r\n.zlp-button-fail {\r\n  background-color: #f44336;\r\n  color: #fff;\r\n}\r\n.zlp-button-wait {\r\n  backgr